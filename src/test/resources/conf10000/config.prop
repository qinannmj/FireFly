stable-storage=d:/jbpaxos

fileOutFacotry-class=cn.com.sparkle.firefly.stablestorage.io.rwsbuffered.RwsBufferedRecordFileOutFactory
#fileOutFacotry-class=cn.com.sparkle.firefly.stablestorage.io.rwbuffered.RwBufferedRecordFileOutFactory
rwsbuffered-file-io-buffsize=5242880
rwsbuffered-file-io-queue-deep=5
async-file-allocator-idle-size=20
async-file-allocator-fileblock-num=8

ip=127.0.0.1
listen-port=10000
listen-port-client=10004
max-response-delay=100
transport-tcp-num=2
transport-singletcp-max-waiting-mem-size=5242880
#net-layer=raptor
net-layer=raptor
disk-mem-lost=0
catchup-delay=5000
debug-log=true
elect-self-master=true
vote-value-split-size=65535
min-instance-merge-package-size=5242880
max-instance-merge-package-size=10485760
transport-timeout=5000
is-merge-client-request=true



net-checksum-type=0
file-checksum-type=3
session-success-sync-max-mem=5242880
session-success-sync-max-mem-strategy=closeConnectionAfterWaitTimeStrategy
session-success-sync-max-mem-waittime=4000
paxos-sender=fullCircleSender
heart-beat-interval=2000
election-priority=1